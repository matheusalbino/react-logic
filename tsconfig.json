{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES5",
    "strict": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "outDir": "dist",
    "declaration": true,
    "declarationDir": "dist",
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
